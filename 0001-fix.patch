+#define CAM_ISP_TFE_DSP_MODE_ROUND                  2
+
+/* Per Path Usage Data */
+#define CAM_ISP_TFE_USAGE_INVALID     0
+#define CAM_ISP_TFE_USAGE_LEFT_PX     1
+#define CAM_ISP_TFE_USAGE_RIGHT_PX    2
+#define CAM_ISP_TFE_USAGE_RDI         3
+
