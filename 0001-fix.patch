+#define CAM_ISP_TFE_IN_LANE_TYPE_DPHY               0
+#define CAM_ISP_TFE_IN_LANE_TYPE_CPHY               1
+#define CAM_ISP_TFE_IN_LANE_TYPE_MAX                2
+
