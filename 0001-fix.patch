+#define CAM_ISP_TFE_GENERIC_BLOB_TYPE_HFR_CONFIG          0
+#define CAM_ISP_TFE_GENERIC_BLOB_TYPE_CLOCK_CONFIG        1
+#define CAM_ISP_TFE_GENERIC_BLOB_TYPE_BW_CONFIG_V2        2
